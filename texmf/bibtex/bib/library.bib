journal = {arXiv preprint arXiv:1411.3998},
keywords = {accretion,accretion discs - mhd,statistical,turbulence - methods},
month = {nov},
number = {November},
pages = {1--17},
title = {{On-off intermittency and amplitude-phase synchronization in Keplerian shear flows}},
url = {http://arxiv.org/abs/1411.3998},
volume = {17},
year = {2014}
}
