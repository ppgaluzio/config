isbn = {1539-3755 (Print)$\backslash$r1539-3755 (Linking)},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {3},
pages = {1--6},
pmid = {11909228},
primaryClass = {nlin},
title = {{Turbulence in small-world networks}},
volume = {65},
year = {2002}
}
